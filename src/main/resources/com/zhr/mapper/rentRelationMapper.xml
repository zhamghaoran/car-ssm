<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zhr.mapper.rentRelationMapper">
    <select id="getRentList" resultType="RentRelation">
        select *
        from rentrelation;
    </select>
    <select id="selectRentListByUsername" resultType="RentRelation">
        select *
        from rentrelation
        where username = #{username}
    </select>
    <insert id="Insert">
        insert into rentrelation values (#{username},#{carType},#{car_id})
    </insert>
    <delete id="Delete">
        delete from rentrelation where username = #{username} and car_type = #{carType} and car_id = #{car_id}
    </delete>
</mapper>